<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.recam">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RECAM">
        <activity android:name=".screens.creditplanning.CreditPlanningActivity"></activity>
        <activity android:name=".screens.aboutus.AboutUsActivity" />
        <activity android:name=".screens.termcondition.TermConditionActivity" />
        <activity android:name=".screens.videoplayscreen.VideoPlayActivity" />
        <activity android:name=".screens.feedback.FeedbackActivity" />
        <activity android:name=".screens.profile.ProfileActivity" />
        <activity android:name=".screens.otp.OtpActivity" />
        <activity android:name=".screens.registration.RegistrationActivity" />
        <activity android:name=".screens.login.LoginActivity" />
        <activity android:name=".screens.categoryvideolist.CategoryVideoList" />
        <activity android:name=".screens.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screens.home.HomeActivity" />
    </application>

</manifest>